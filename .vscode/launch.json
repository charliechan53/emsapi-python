        {
            "name": "Python: example.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/example.py",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: clean_swagger.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/tools/clean_swagger.py",
            "args": [
                "--file", "${workspaceFolder}/ems-api.json"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: autorest_patch.py",
            "type": "python",
